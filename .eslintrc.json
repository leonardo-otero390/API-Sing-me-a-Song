{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "quotes":"warn",
        "import/prefer-default-export": "off",
        "import/extensions": [
        "error",
        {
            "js": "ignorePackages"
        }
       ]
    },
    "ignorePatterns": ["*.test.js"]
}
